{
	"schemaVersion": 1,
	"id": "proto-quests",
	"version": "${version}",
	"name": "ProtoQuests",
	"description": "New questing mod with RPG (story) like style. A Fabric version for questing plugins.",
	"authors": [
		"WooksCode"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "GPL-3.0",
